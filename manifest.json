{
    "name": "Shiro Tabs Utility",
    "version": "1.2",
    "description": "Useful util for group replacing url.",
    "permissions": ["storage", "tabs", "clipboardWrite"],
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [],
    "options_ui": {
        "page": "options.html"
    },
    "icons": {
        "48": "48.png",
        "96": "96.png"
    }
}
